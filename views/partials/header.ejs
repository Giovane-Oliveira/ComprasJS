<!doctype html>
<html lang="pt-br">

<head>
  <title>Compras</title>
  <meta charset="utf-8">
  <link rel="stylesheet" href="/assets/css/style.css">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

  <link href="https://fonts.googleapis.com/css?family=Poppins:300,400,500,600,700,800,900" rel="stylesheet">

  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="../../css/style.css">

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.mask/1.14.16/jquery.mask.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-maskmoney/3.0.2/jquery.maskMoney.min.js" type="text/javascript"></script>

  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.0.0/dist/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">

  <script src="https://cdn.datatables.net/2.0.2/js/dataTables.js"></script>
  <script src="https://cdn.datatables.net/2.0.2/css/dataTables.dataTables.css"></script>

  <!-- switch button bootstrap -->
  <link href="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.6.1/css/bootstrap4-toggle.min.css" rel="stylesheet">
  <script src="https://cdn.jsdelivr.net/gh/gitbrent/bootstrap4-toggle@3.6.1/js/bootstrap4-toggle.min.js"></script>

  <link rel="stylesheet" href="../../css/estiloPagamentos.css">
  <link rel="stylesheet" href="../../css/estiloCompras.css">
  <link rel="stylesheet" href="../../css/sidebar.css">

  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.3/jquery.validate.min.js"></script>


</head>

<body>

  <div class="wrapper d-flex align-items-stretch">
    <nav id="sidebar">
      <div class="custom-menu">
        <button type="button" id="sidebarCollapse" class="btn btn-primary">
          <i class="fa fa-bars"></i>
          <span class="sr-only">Toggle Menu</span>
        </button>
      </div>
      <ul class="list-unstyled components mb-5">
        <div id="userIcon">
          <li class="active">
            <a href="/dashboard">
              <img src="../images/user.png">
              <br>
              <span>
                <%= user.employee.name %>
              </span>
            </a>
          </li>
        </div>
        <li>
          <div class="dashimg"> <a href="/dashboard"><img src="../images/icondash.png">Dashboard</a></div>
        </li>
        <li>
          <div class="dashimg"><a href="/purchases"><img src="../images/carrinho.png"> Compras/Serviços</a></div>
        </li>
        <li>
          <div class="dashimg"><a href="/payments"><img src="../images/dolar.png">Pagamentos</a></div>
        </li>
        <% if(user.permissions.user_registration==1){ %>
          <li>
            <div class="dashimg"><a href="/registrations_token"><img src="../images/useradd.png">Cadastro</a></div>
          </li>
          <li>
            <div class="dashimg"><a href="/users"><img src="../images/users.png">Usuários</a></div>
          </li>
          <% } %>

            <% if(user.permissions.supplier_registration==1){ %>
              <li>
                <div class="dashimg"><a href=" /suppliers"><img src="../images/pacote.png">Fornecedores</a></div>
              </li>
              <li>
                <div class="dashimg"><a href=" /suppliers_register"><img src="../images/addforn.png">Cadastrar Fornecedor</a></div>
              </li>
              <% } %>
                <li>
                  <div class="dashimg"><a href=" /logout"><img src="../images/logout.png">Logout</a></div>
                </li>
      </ul>
    </nav>
    <script type="module">
      import Typebot from 'https://cdn.jsdelivr.net/npm/@typebot.io/js@0.3/dist/web.js'

      Typebot.initBubble({
        typebot: "compras-syhrwnh",
        theme: {
          button: { backgroundColor: "#00aa9b" },
          chatWindow: { backgroundColor: "#fff" },
        },
      });
    </script>
    <div id="content" class="p-4 p-md-5 pt-5">
      <!-- Page Content  -->